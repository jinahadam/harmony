<Window x:Class="harmony.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="383.485" Width="738.046">
    <Grid>
        <Button Content="Calibrate " HorizontalAlignment="Left" Margin="25,23,0,0" VerticalAlignment="Top" Width="187" Click="Button_Click" Height="39"/>
        <Label x:Name="calibrationLabel" Content="Calibration Info&#xA;" HorizontalAlignment="Left" Margin="228,23,0,0" VerticalAlignment="Top" Height="39" Width="591" RenderTransformOrigin="0.5,0.5" FontSize="20">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.25"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>

    </Grid>
</Window>
